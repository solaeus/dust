let mut count = 1;

while count <= 15 {
	let divides_by_3 = count % 3 == 0;
	let divides_by_5 = count % 5 == 0;

	if count % 3 == 0 && count % 5 == 0 {
		write_line("fizzbuzz")
	} else if count % 3 == 0 {
		write_line("fizz")
	} else if count % 5 == 0 {
		write_line("buzz")
	} else {
	    write_line(to_string(count))
	}

	count += 1
}
